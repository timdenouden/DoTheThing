  "runtimeVersion": "V8",
  "addOns": {
    "common": {
      "name": "Do The Thing!",
      "logoUrl": "https://timdenouden.github.io/DoTheThing/images/baseline_check_circle_outline_black_48dp.png",
      "layoutProperties": {
        "primaryColor": "#212121",
        "secondaryColor": "#2e7d32"
      },
      "useLocaleFromApp": true,
      "homepageTrigger": {
        "runFunction": "onHomepage",
        "enabled": true
      }
    },
    "calendar": {
      "eventOpenTrigger": {
        "runFunction": "onCalendarEventOpen"
